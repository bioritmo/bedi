require 'redecard/header'
require 'redecard/entry_01'
require 'redecard/entry_30'
require 'redecard/entry_40'
require 'redecard/trailer'

grammar Bedi::Redecard::Parser
  include Bedi::Redecard::Header
  include Bedi::Redecard::Entry01
  include Bedi::Redecard::Entry30
  include Bedi::Redecard::Entry40
  include Bedi::Redecard::Trailer

  rule Parser
    Header
    Entry01*
    Entry30
    Entry40
    Trailer
  end
end
